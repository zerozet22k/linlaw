      "application/x-eps",
      "application/postscript",
      "image/x-adobe-dng",
      "image/x-canon-cr2",
      "image/x-nikon-nef",
      "image/x-sony-arw",
      "image/x-olympus-orf",
      "image/x-fuji-raf",
      "image/x-panasonic-rw2",
